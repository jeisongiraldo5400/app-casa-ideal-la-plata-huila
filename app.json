{
  "expo": {
    "name": "Casa Ideal",
    "slug": "app-casa-ideal",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "appcasaideal",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/icono.png",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icono.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "imageWidth": 200
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.casaideal.laplatahuila",
      "icon": "./assets/images/icono.png"
    },
    "android": {
      "icon": "./assets/images/icono.png",
      "adaptiveIcon": {
        "backgroundColor": "#1e3a8a",
        "foregroundImage": "./assets/images/icono.png"
      },
      "splash": {
        "image": "./assets/images/icono.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "imageWidth": 200
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.casaideal.laplatahuila"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icono.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icono.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "imageWidth": 200
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir acceso a la cámara para escanear códigos de barras."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e4ea88b1-ecc0-4487-897f-d28f2f7eb407"
      }
    }
  }
}
